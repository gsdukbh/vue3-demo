{
  "name": "vite-ssr-demo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "node server",
    "test:unit": "vue-cli-service test:unit",
    "buildssr": "yarn buildssr:client && yarn buildssr:server",
    "buildssr:client": "vite build --ssrManifest --outDir dist/client",
    "buildssr:server": "vite build --ssr src/server.js --outDir dist/server",
    "debug": "node --inspect-brk server",
    "generate": "vite build --ssrManifest --outDir dist/static && yarn buildssr:server && node prerender",
    "servessr": "cross-env NODE_ENV=production node server"
  },
  "dependencies": {
    "vue": "^3.0.5",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/cli-service": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/server-renderer": "^3.0.6",
    "@vue/test-utils": "^2.0.0-0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "serve-static": "^1.14.1",
    "typescript": "^4.1.3",
    "vite": "^2.3.3",
    "vue-jest": "^5.0.0-0",
    "vue-tsc": "^0.0.24"
  }
}
